{
  "name": "deeplint",
  "description": "Automated cloud security and excellence scanner",
  "version": "0.4.0",
  "author": "(Gavin Yue @gavinyue)",
  "bin": {
    "deeplint": "./bin/run"
  },
  "bugs": "https://github.com/deeplint/deeplint/issues",
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^2.2.3",
    "ajv": "^6.12.2",
    "chalk": "^4.0.0",
    "cli-table": "^0.3.1",
    "figures": "^3.2.0",
    "handlebars": "^4.7.6",
    "isomorphic-git": "^1.4.0",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.15",
    "tslib": "^1.13.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "@types/chai": "^4.2.11",
    "@types/js-yaml": "^3.12.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.24",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^7.2.0",
    "nyc": "^14.1.1",
    "ts-json-schema-generator": "^0.68.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/schema/generated",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/deeplint/deeplint",
  "keywords": [
    "deeplint",
    "cloud",
    "security",
    "policy"
  ],
  "license": "MPL-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "deeplint",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "deeplint/deeplint",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && rm -rf schema/generated/*.* && node schema/generator.js && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
