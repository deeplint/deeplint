{
  "name": "@stacklint/cli",
  "description": "StackLint",
  "version": "0.1.0",
  "author": "(Gavin Yue @gavinyue)",
  "bin": {
    "stacklint": "./bin/run"
  },
  "bugs": "https://github.com/stacklint/stacklint/issues",
  "dependencies": {
    "@oclif/command": "^1.6.0",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^2.2.3",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.21",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^7.1.2",
    "nyc": "^14.1.1",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/stacklint/stacklint",
  "keywords": [
    "oclif"
  ],
  "license": "MPL-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "stacklint",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "stacklint/stacklint",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
